<h1>Listing workflows</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @workflows.each do |workflow| %>
  <tr>
    <td><%=h workflow.name %></td>
    <td><%= link_to 'Show', workflow %></td>
    <td><%= link_to 'Edit', edit_workflow_path(workflow) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New workflow', new_workflow_path %>