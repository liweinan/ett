<% unless package.blank? %>
    <%
       begin
         package.reload
       rescue
         # do nothing on ActiveRecord::RecordNotFound
       end
    %>
    <% ub = package.upgrade_bz %>
    <% package.bz_bugs.each do |bz| %>
        <% unless ub.include? bz %>
            <%= link_to bz.bz_id, "#{APP_CONFIG['bugzilla_prefix']}#{bz.bz_id}", :target => '_blank' %>
        <% end %>
    <% end %>
<% end %>
