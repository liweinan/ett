           :locals => {:package => package,
                       :field_name => 'rpmdiff',
                       :field_type => 'packages/index/field_input',
                       :partial => 'packages/index/rpmdiff_col',
                       :style => package.rpmdiff_style(distro)} %>
<% end %>
