<thead>
<tr>
  <% if logged_in? && (!has_task? || !Task.readonly?(Task.find_by_name(unescape_url(params[:task_id])))) %>
      <<%= col %> class='content-list-head'>Operations</<%= col %>>
  <% end %>
  <<%= col %> data-priority="1" class='content-list-head'>Name</<%= col %>>
  <<%= col %> data-priority="2" class='content-list-head'>Status</<%= col %>>
  <<%= col %> data-priority="3" class='content-list-head'>Tags</<%= col %>>
  <<%= col %> data-priority="4" class='content-list-head'>Assignee</<%= col %>>
  <<%= col %> data-priority="5" class='content-list-head'>Version</<%= col %>>
  <% if @task.use_bz_integration? %>
      <<%= col %> data-priority="6" class='content-list-head'>Errata BZ</<%= col %>>
      <<%= col %> data-priority="7" class='content-list-head'>BZ</<%= col %>>
  <% end %>
  <<%= col %> data-priority="8" class='content-list-head'>GitUrl</<%= col %>>
  <% if @task.use_mead_integration? %>
      <<%= col %> data-priority="9" class='content-list-head'>Mead</<%= col %>>
      <<%= col %> data-priority="10" class='content-list-head'>Brew <%= params.include?(:brew) ? "(#{params[:brew]})": ''%></<%= col %>>
      <<%= col %> data-priority="11" class='content-list-head'>Rpm Diff</<%= col %>>
  <% end %>
</tr>
</thead>
