<thead>
<tr>
  <% if logged_in? %>
      <<%= col %> class='content-list-head'>Operations</<%= col %>>
  <% end %>
  <<%= col %> data-priority="1" class='content-list-head'>Name</<%= col %>>
  <<%= col %> data-priority="2" class='content-list-head'>Status</<%= col %>>
  <<%= col %> data-priority="3" class='content-list-head'>Tags</<%= col %>>
  <<%= col %> data-priority="4" class='content-list-head'>Assignee</<%= col %>>
  <<%= col %> data-priority="5" class='content-list-head'>Version</<%= col %>>
  <<%= col %> data-priority="6" class='content-list-head'>Previous Version</<%= col %>>
  <<%= col %> data-priority="7" class='content-list-head'>Milestone</<%= col %>>
  <% if @task.use_bz_integration? %>
      <<%= col %> data-priority="8" class='content-list-head'>Errata BZ</<%= col %>>
      <<%= col %> data-priority="9" class='content-list-head'>BZ</<%= col %>>
      <<%= col %> data-priority="10" class='content-list-head'>Jira</<%= col %>>
  <% end %>
  <<%= col %> data-priority="11" class='content-list-head'>Github PR</<%= col %>>
  <% if @task.has_pkg_with_optional_errata? %>
      <<%= col %> data-priority="12" class='content-list-head'>Async Erratum</<%= col %>>
  <% end %>
  <<%= col %> data-priority="13" class='content-list-head'>GitUrl</<%= col %>>
  <% if @task.use_mead_integration? %>
      <<%= col %> data-priority="14" class='content-list-head'>Mead</<%= col %>>
      <<%= col %> data-priority="15" class='content-list-head'>Brew <%= params.include?(:brew) ? "(#{params[:brew]})": ''%></<%= col %>>
      <<%= col %> data-priority="16" class='content-list-head'>Rpm Diff</<%= col %>>
  <% end %>
</tr>
</thead>
